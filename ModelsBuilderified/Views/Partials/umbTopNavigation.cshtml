@inherits UmbracoViewPage<INavigation>
<!-- Nav -->
<nav id="nav" class="skel-panels-fixed">
    <ul>
        @foreach (var item in Model.MenuItems)
        {
            <li class="@(Model.Id == item.Id ? "current_page_item" : null)"><a href="@item.Url">@item.Name</a></li>
        }
    </ul>
</nav>
<!-- /Nav -->